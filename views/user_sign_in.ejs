<link rel="stylesheet" href="/css/user_sign_in.css" />
<div id="sign-in-page">
  <form action="/users/create-session" method="POST">
    <h2>Sign in</h2>
    <p id="google-sign-in">
      <a href="/users/auth/google"
        ><img src="/images/google.png" height="30" width="35" /><span>
          Sign In with Google</span
        ></a
      >
    </p>
    <input name="email" type="email" placeholder="Your Email-id" required />
    <input name="password" type="password" placeholder="Password" required />
    <input type="submit" value="Sign In" />
    <p id="already-account">
      <span>Don't have an account?</span>
      <a href="/users/sign-up">Sign Up</a>
    </p>

    <p></p>
  </form>
</div>
